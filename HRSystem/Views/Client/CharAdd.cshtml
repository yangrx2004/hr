@{
    Layout = null;
}
@using Model
@model Config_public_char
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <link href="~/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
</head>

<body >
    <table width="100%">
        <tr>
            <td>
                <font color="black">
                    您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--公共属性设置--公共属性添加
                </font>
            </td>
        </tr>
        
       
    </table>

    <div class="container">

        @using (Html.BeginForm("CharAdd", "Client", FormMethod.Post))
        {
            <table width="100%">
                <tr>
                    <td align="right">
                        <input type="submit" value="提交" class="BUTTON_STYLE1">
                        <input type="button" value="返回" class="BUTTON_STYLE1"
                               onclick="history.back();">
                    </td>
                </tr>
            </table>
            <div class="form-group">
                @Html.LabelFor(e => e.Attribute_kind)
                @Html.TextBoxFor(e => e.Attribute_kind, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(e => e.Attribute_name)
                @Html.TextBoxFor(e => e.Attribute_name, new { @class = "form-control" })
            </div>
            
        }

    </div>

</body>
</html>