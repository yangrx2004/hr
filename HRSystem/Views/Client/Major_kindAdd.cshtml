@{
    Layout = null;
}
@using Model
@model Config_major_kind
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <title>MajorAdd</title>

    <script type="text/javascript" src="../../javascript/jquery-1.7.2.js"></script>
    <link href="~/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
</head>
<body>


    <table width="100%">
        <tr>
            <td>
                <font color="black">
                    您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职位分类设置--职位分类设置添加
                </font>
            </td>
        </tr>
    </table>


    <div class="container">

        @using (Html.BeginForm("Major_kindAdd", "Client", FormMethod.Post))
        {
            <table width="100%">
                <tr>
                    <td align="right">
                        <input type="submit" value="提交" class="BUTTON_STYLE1">
                        <input type="button" value="返回" class="BUTTON_STYLE1"
                               onclick="history.back();">
                    </td>
                </tr>
            </table>
            <div class="form-group">
                @Html.LabelFor(e => e.Major_kind_id)
                @Html.TextBoxFor(e => e.Major_kind_id, new { @class = "form-control", disabled = "disabled " })
               
            </div>
            <div class="form-group">
                @Html.LabelFor(e => e.Major_kind_name)
                @Html.TextBoxFor(e => e.Major_kind_name, new { @class = "form-control" })
            </div>
            
        }

    </div>

</body>
</html>
