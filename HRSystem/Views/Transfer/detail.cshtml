
@{
    Layout = null;
}
@using Model;
@{ major_change mc = ViewBag.s;}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet" href="../../css/table.css"
          type="text/css"></link>
    <script src="~/Scripts/vue/dist/vue.js"></script>
    <script src="~/Scripts/element-ui/lib/index.js"></script>
    <link href="~/Scripts/element-ui/lib/theme-chalk/index.css" rel="stylesheet" />
    <script src="~/Scripts/axios/dist/axios.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <style>
        .TD_STYLE2 {
            BORDER: 1PX SOLID;
        }
    </style>
</head>

<body>
    <table width="100%">
        <tr>
            <td>
                <font color="black">您正在做的业务是:人力资源--培训管理--调动登记查看</font>
            </td>
        </tr>
        <tr>
            <td align="right">
                <input type="button" class="BUTTON_STYLE1" onclick="javascript:window.history.back();" value="返回">
            </td>
        </tr>
    </table>
    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr height="21">
            <td class="TD_STYLE1" width="8%">
                档案编号
            </td>
            <td class="TD_STYLE2" width="10%">
                @mc.human_id
            </td>
            <td class="TD_STYLE1" width="8%">
                姓名
            </td>
            <td class="TD_STYLE2" width="10%">
                @mc.human_name
            </td>
            <td class="TD_STYLE1" width="8%">
            </td>
            <td class="TD_STYLE2" width="10%">
            </td>
            <td class="TD_STYLE1" width="8%">
            </td>
            <td class="TD_STYLE2" width="10%">
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1" width="8%">
                原一级机构
            </td>
            <td class="TD_STYLE2">
                @mc.first_kind_name
            </td>
            <td class="TD_STYLE1" width="8%">
                原二级机构
            </td>
            <td class="TD_STYLE2" width="10%">
                @mc.second_kind_name
            </td>
            <td class="TD_STYLE1" width="8%">
                原三级机构
            </td>
            <td class="TD_STYLE2" width="10%">
                @mc.third_kind_name
            </td>
            <td class="TD_STYLE1" width="8%">
            </td>
            <td class="TD_STYLE2" width="10%">
            </td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                原职位分类
            </td>
            <td class="TD_STYLE2">
                @mc.major_kind_name
            </td>
            <td class="TD_STYLE1" width="12%">
                原职位名称
            </td>
            <td class="TD_STYLE2">
               @mc.major_name
            </td>
            <td class="TD_STYLE1">
                原薪酬标准
            </td>
            <td class="TD_STYLE2">
                @mc.salary_standard_name
            </td>
            <td class="TD_STYLE1">
                原薪酬总额
            </td>
            <td class="TD_STYLE2">
                @mc.salary_sum
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                新一级机构名称
            </td>
            <td class="TD_STYLE2">
                <select name="mc.newFirstKindId" class="INPUT_STYLE2">
                    <option value="1353318953319">@mc.new_first_kind_name</option>
                </select>

            </td>
            <td class="TD_STYLE1" width="12%">
                新二级机构名称
            </td>
            <td class="TD_STYLE2">
                <select name="mc.newSecondKindId" class="INPUT_STYLE2">
                    <option value="1353318979558">@mc.new_second_kind_name</option>
                </select>
            </td>
            <td class="TD_STYLE1" width="12%">
                新三级机构名称
            </td>
            <td class="TD_STYLE2">
                <select name="mc.newThirdKindId" class="INPUT_STYLE2">
                    <option value="1353319077370">@mc.new_third_kind_name</option>
                </select>
            </td>
            <td class="TD_STYLE1" width="8%">
            </td>
            <td class="TD_STYLE2" width="10%">
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                新职位分类
            </td>
            <td class="TD_STYLE2">
                <select name="mc.newMajorKindId" class="INPUT_STYLE2">
                    <option value="02">@mc.new_major_kind_name</option>
                </select>
            </td>
            <td class="TD_STYLE1" width="12%">
                新职位名称
            </td>
            <td class="TD_STYLE2">
                <select name="mc.newMajorId" class="INPUT_STYLE2">
                    <option value="02">@mc.new_major_name</option>
                </select>
            </td>
            <td class="TD_STYLE1">
                新薪酬标准
            </td>
            <td class="TD_STYLE2">
                <select name="mc.newSalaryStandardId" class="INPUT_STYLE2">
                    <option value="1353320082662">@mc.new_salary_standard_name</option>
                </select>
            </td>
            <td class="TD_STYLE1">
                新薪酬总额
            </td>
            <td class="TD_STYLE2">
                @mc.new_salary_sum
            </td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                登记人
            </td>
            <td class="TD_STYLE2">
               @mc.register
            </td>
            <td class="TD_STYLE1">
                登记时间
            </td>
            <td class="TD_STYLE2">
               @mc.regist_time
            </td>
            <td class="TD_STYLE1">
                复核人
            </td>
            <td class="TD_STYLE2">
               @mc.checker
            </td>
            <td class="TD_STYLE1">
                复核时间
            </td>
            <td class="TD_STYLE2">
               @mc.check_time
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                调动原因
            </td>
            <td colspan="7" class="TD_STYLE2">
                <textarea name="majorChange.changeReason" rows="6" readonly="readonly" class="TEXTAREA_STYLE1">						
						@mc.change_reason
						</textarea>
            </td>
        </tr>
    </table>

</body>
</html>



